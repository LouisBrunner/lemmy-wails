    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "2.7.1",
    "react": "^18.2.0",
