    "autoprefixer": "^10.4.12",
    "daisyui": "^2.31.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-daisyui": "^2.4.2",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.4.1",
